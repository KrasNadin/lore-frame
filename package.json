{
  "name": "test-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "storybook": "storybook dev -p 3000",
    "nuke": "rm -rf node_modules yarn.lock && yarn cache clean --all && yarn && yarn upgrade",
    "precommit": "lint-staged",
    "prepush": "yarn build",
    "start": "vite",
    "build": "vite build",
    "lint": "eslint src --ext ts,tsx,js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx,js,jsx --fix --format=compact --report-unused-disable-directives",
    "format": "prettier --log-level warn --write \"src/**/*.{jsx,js,ts,tsx}\"",
    "preview": "vite preview",
    "prepare": "husky install",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^1.2.0",
    "babel-polyfill": "^6.26.0",
    "chart.js": "^3.9.1",
    "classnames": "^2.2.6",
    "history": "^4.7.2",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.0",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@storybook/addon-actions": "^7.2.1",
    "@storybook/addon-essentials": "^7.2.1",
    "@storybook/addon-interactions": "^7.2.1",
    "@storybook/addon-links": "^7.2.1",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.2.1",
    "@storybook/builder-vite": "^7.2.1",
    "@storybook/builder-webpack5": "^7.2.1",
    "@storybook/cli": "^7.2.1",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/node-logger": "^7.2.1",
    "@storybook/preset-create-react-app": "^7.2.1",
    "@storybook/react": "^7.2.1",
    "@storybook/react-vite": "^7.2.1",
    "@storybook/react-webpack5": "^7.2.1",
    "@storybook/testing-library": "^0.2.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/i18next": "^13.0.0",
    "@types/i18next-browser-languagedetector": "^2.0.0",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.195",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/react-i18next": "^8.1.0",
    "@types/react-redux": "^7.1.25",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-slick": "^0.23.10",
    "@types/uuid": "^9.0.1",
    "@types/youtube-player": "^5.5.9",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "@vitejs/plugin-react": "^4.0.1",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "babel-jest": "^29.7.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "eslint": "^8.45.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-clean-code": "^0.1.12",
    "eslint-plugin-i18next": "^6.0.3",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-storybook": "^0.6.13",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^13.2.3",
    "npm-run-all": "^4.1.3",
    "prettier": "^3.0.0",
    "request-promise": "^4.2.4",
    "sass": "^1.64.0",
    "storybook": "^7.2.1",
    "ts-jest": "^29.1.2",
    "typescript": "^5.0.2",
    "vite": "^4.4.0",
    "vite-plugin-eslint": "^1.8.1",
    "webpack": "^5.88.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --ext .ts,.tsx,.js --fix"
    ]
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
